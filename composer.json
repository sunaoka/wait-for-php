{
    "name": "sunaoka/wait-for-php",
    "description": "Wait for a some process to be ready",
    "keywords": ["PHP", "Wait", "Ready"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-sockets": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\WaitFor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
